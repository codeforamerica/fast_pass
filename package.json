{
  "name": "lv-fastpass",
  "description": "Las Vegas Development Fastpass",
  "version": "0.0.1",
  "private": true,
  "repository": {
      "type": "git",
      "url": "https://github.com/rclosner/lv-dof"
  },
  "scripts": {
    "start": "node app.js",
    "db-migrate": "db-migrate up --config config/database.json --migrations-dir db/migrations -e \"$NODE_ENV\"",
    "db-rollback": "db-migrate down --config config/database.json --migrations-dir db/migrations -e \"$NODE_ENV\"",
    "db-create": "psql -c \"CREATE DATABASE fast_track_$NODE_ENV;\"",
    "db-setup": "cp config/database.json.sample config/database.json; npm run-script db-create; npm run-script db-migrate;"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "dependencies": {
    "express": "3.2.6",
    "pg": "2.2.0",
    "jade": "*",
    "less-middleware": "0.1.x",
    "proj4": "1.5.x"
  }
}
